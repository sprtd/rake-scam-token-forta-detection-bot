{
  "name": "rake-scam-token-detection-bot",
  "version": "0.0.1",
  "description": "Detects rake scam tokens which significantly take additional swap fee on Uniswap DEX and transfers it to an EOA",
  "repository": "https://github.com/sprtd/rake-scam-token-forta-detection-bot.git",
  "chainIds": [
    1,
    137,
    42161,
    10,
    43114,
    42220,
    56
  ],
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec \"npm run build && forta-agent run\"",
    "start:prod": "forta-agent run --prod",
    "tx": "npm run build && forta-agent run --tx",
    "block": "npm run build && forta-agent run --block",
    "range": "npm run build && forta-agent run --range",
    "alert": "npm run build && forta-agent run --alert",
    "sequence": "npm run build && forta-agent run --sequence",
    "file": "npm run build && forta-agent run --file",
    "publish": "forta-agent publish",
    "info": "forta-agent info",
    "logs": "forta-agent logs",
    "push": "forta-agent push",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@ethersproject/solidity": "^5.7.0",
    "@uniswap/v2-core": "^1.0.1",
    "axios": "^1.3.4",
    "bignumber.js": "^9.1.1",
    "ethers": "^6.0.8",
    "forta-agent": "^0.1.18"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/nodemon": "^1.19.0",
    "dotenv": "^16.0.3",
    "forta-agent-tools": "^3.2.6",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8",
    "prettierrc": "^0.0.0-5",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
